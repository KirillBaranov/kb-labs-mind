{
  "name": "@kb-labs/mind-cli",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "CLI commands for KB Labs Mind",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./application/sync": {
      "import": "./dist/application/sync.js",
      "types": "./dist/application/sync.d.ts"
    },
    "./studio/widgets/query-widget": {
      "import": "./dist/studio/widgets/query-widget.js",
      "types": "./dist/studio/widgets/query-widget.d.ts"
    },
    "./studio/widgets/verify-widget": {
      "import": "./dist/studio/widgets/verify-widget.js",
      "types": "./dist/studio/widgets/verify-widget.d.ts"
    },
    "./dist/*": "./dist/*"
  },
  "kb": {
    "manifest": "./dist/manifest.v3.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "scripts": {
    "pretype-check": "pnpm --filter @kb-labs/shared-cli-ui build && pnpm --filter @kb-labs/mind-gateway build",
    "clean": "rimraf dist",
    "build": "tsup --config tsup.config.ts",
    "dev": "tsup --config tsup.config.ts --watch",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@byjohann/toon": "0.4.0",
    "@kb-labs/core-runtime": "link:../../../kb-labs-core/packages/core-runtime",
    "@kb-labs/plugin-contracts": "link:../../../kb-labs-plugin/packages/plugin-contracts",
    "@kb-labs/sdk": "link:../../../kb-labs-sdk/packages/sdk",
    "@kb-labs/mind-contracts": "link:../mind-contracts",
    "@kb-labs/mind-core": "link:../mind-core",
    "@kb-labs/mind-engine": "link:../mind-engine",
    "@kb-labs/mind-indexer": "link:../mind-indexer",
    "@kb-labs/mind-llm": "link:../mind-llm",
    "@kb-labs/mind-orchestrator": "link:../mind-orchestrator",
    "@kb-labs/mind-types": "link:../mind-types",
    "fast-glob": "^3.3.2",
    "react": "^18.3.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^24.3.3",
    "@types/react": "^18.3.8",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.11.0"
}

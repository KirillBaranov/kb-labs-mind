  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@types/node": "^20.0.0",
    "rimraf": "^6",
    "tsup": "^8",
    "typescript": "^5",
    "vitest": "^3.2.4"

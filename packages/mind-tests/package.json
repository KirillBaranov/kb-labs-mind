{
  "name": "@kb-labs/mind-tests",
  "version": "0.1.0",
  "private": true,
  "description": "Test suite for KB Labs Mind",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:fixtures": "vitest run --reporter=verbose fixtures",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ignore-pattern 'dist/**'",
    "lint:fix": "eslint . --fix --ignore-pattern 'dist/**'"
  },
  "dependencies": {
    "@kb-labs/mind-cli": "link:../mind-cli",
    "@kb-labs/mind-indexer": "link:../mind-indexer",
    "vitest": "^3.2.4"
  },
  "devDependencies": {
    "@kb-labs/devkit": "link:../../../kb-labs-devkit",
    "@vitest/coverage-v8": "^3",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.11.0"
}
